#!/bin/bash -e

if [ ${ES_VERSION} ]; then
        echo $ES_VERSION
else
        echo 1.5.0
fi > $OPENSHIFT_ELASTICSEARCH_DIR/env/ES_VERSION

if [ ${KIBANA_VERSION} ]; then
        echo $KIBANA_VERSION
else
        echo 4.0.2
fi > $OPENSHIFT_ELASTICSEARCH_DIR/env/KIBANA_VERSION

if [ ${OPENSHIFT_ELASTICSEARCH_KIBANA_PROXY_PORT} ]; then
        echo $OPENSHIFT_ELASTICSEARCH_KIBANA_PROXY_PORT
else
        echo
fi > $OPENSHIFT_ELASTICSEARCH_DIR/env/KIBANA_PORT
